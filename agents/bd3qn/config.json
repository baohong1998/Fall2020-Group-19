{
  "environment": {
    "env_name": "everglades-v0",
    "config_dir": "../../config/",
    "map_file": "DemoMap.json",
    "setup_file": "GameSetup.json",
    "unit_file": "UnitDefinitions.json",
    "env_output_dir": "../../game_telemetry"
  },
  "exploration": {
    "epsilon_start": 1,
    "epsilon_final": 0.01,
    "epsilon_decay": 20000
  },
  "training": {
    "target_update_freq": 1000,
    "start_learning": 5000,
    "learning_rate": 0.0001,
    "max_steps": 10000000,
    "max_episodes": 25000
  },
  "memory_replay": {
    "capacity": 100000,
    "batch_size": 128
  },
  "model": {
    "temporal_difference_target": "mean",
    "gamma": 0.95,
    "hidden_dim": 128
  },
  "output": {
    "save_update_freq": 20,
    "directory": "Mean",
    "dpi": 72
  },
  "device": "cuda:0"
}