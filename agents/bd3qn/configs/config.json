{
  "environment": {
    "env_name": "everglades-v0",
    "config_dir": "../../config/",
    "map_file": "DemoMap.json",
    "setup_file": "GameSetup.json",
    "unit_file": "UnitDefinitions.json",
    "env_output_dir": "../../game_telemetry"
  },
  "exploration": {
    "epsilon_start": 1,
    "epsilon_final": 0.01,
    "epsilon_decay": 8000
  },
  "training": {
    "target_update_freq": 750,
    "start_learning": 512,
    "learning_rate": 0.0001,
    "max_steps": 10000000,
    "max_episodes": 800
  },
  "memory_replay": {
    "capacity": 2048,
    "batch_size": 32
  },
  "model": {
    "temporal_difference_target": "mean",
    "exploration": "Epsilon",
    "gamma": 0.99,
    "hidden_dim": 128
  },
  "output": {
    "save_update_freq": 20,
    "directory": "Mean",
    "dpi": 72
  },
  "device": "cuda:0"
}
